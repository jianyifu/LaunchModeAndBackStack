<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fjy.activity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.fjy.activity.MainActivity"
            android:label="MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="appOriginal" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fjy.activity.StandardB"
            android:exported="true"
            android:label="StandardB">
            <intent-filter>
                <action android:name="StandardB" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fjy.activity.SingleTopC"
            android:label="SingleTopC"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="SingleTopC" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fjy.activity.SingleTaskD"
            android:label="SingleTaskD"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="SingleTaskD" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fjy.activity.SingleInstanceE"
            android:label="SingleInstanceE"
            android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="SingleInstanceE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>